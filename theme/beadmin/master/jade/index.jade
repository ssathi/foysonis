extends layout/main-layout

block active-item
  - firstMenu['Dashboard']['active'] = 1

block content
  
  h3 
    .pull-right.text-center
      .text-sm.mb-sm 500 ratings
      .inlinesparkline(data-bar-color="#cfdbe2", data-height="18", data-bar-width="3", data-bar-spacing="2") 
        | 2,3,4,7,5,7,8,9,5,7,8,4,7

    | Dashboard
    small Hi, User. Welcome back!

  .hidden-xs(data-toggle="notify", data-onload, data-message="<b>This is notify!</b> Dismiss with a click or wait for it to disappear." data-options='{"status":"warning", "pos":"bottom-right"}')

  .row

    // START dashboard main content
    section.col-md-9

      // START chart
      .row
        .col-lg-12
          // START widget
          .panel.widget.bg-inverse
            .row
              .col-sm-2.text-right
                .row
                  .col-sm-12.col-xs-4
                    .ph
                      p.h3.mb0 11200
                      p.m0.text-muted Visits
                  .col-sm-12.col-xs-4
                    .ph
                      p.h3.mb0 2000
                      p.m0.text-muted Likes
                  .col-sm-12.col-xs-4
                    .ph
                      p.h3.mb0 3500
                      p.text-muted Subscriptions
              .col-sm-10.bg-primary
                .p-lg
                  .h5.mt0 Latest stadistics
                  // Line chart
                  .inlinesparkline.sparkline(data-type='line', data-height='100', data-width='100%', data-line-width='2', data-line-color='#dddddd', data-spot-color='#bbbbbb', data-fill-color='', data-highlight-line-color='#fff', data-spot-radius='3', data-resize='true')
                    // 1,5,3,6,5,11,2,4,5,7,9,6,4
                  // Bar chart
                  .text-center
                    .inlinesparkline.inline(data-bar-color="#fff", data-height="60", data-bar-width="8", data-bar-spacing="6") 
                      | 1,5,3,6,5,8,2,4,5,7,9,6,4,3,6,5,9,2
          // END widget
      // END chart
      .row
        .col-md-4
          // START widget
          .panel.widget(data-toggle="play-animation", data-play="fadeInLeft", data-offset="0", data-delay="100")
            .panel-body.bg-primary
              .row.row-table.row-flush
                .col-xs-8
                  p.mb0 New connections
                  h3.m0 150
                .col-xs-4.text-right
                  em.fa.fa-share-alt.fa-2x
            .panel-body
              // Bar chart
              .text-center
                .inlinesparkline.inline(data-bar-color="primary", data-height="30", data-bar-width="6", data-bar-spacing="6") 
                  | 5,3,4,6,5,9,4,4,10,5,9,6,4
        .col-md-4
          // START widget
          .panel.widget(data-toggle="play-animation", data-play="fadeInDown", data-offset="0", data-delay="100")
            .panel-body.bg-success
              .row.row-table.row-flush
                .col-xs-8
                  p.mb0 Uploads
                  h3.m0 540
                .col-xs-4.text-right
                  em.fa.fa-cloud-upload.fa-2x
            .panel-body
              // Bar chart
              .text-center
                .inlinesparkline.inline(data-bar-color="success", data-height="30", data-bar-width="6", data-bar-spacing="6") 
                  | 10,30,40,70,50,90,70,50,90,40,40,60,40
        .col-md-4
          // START widget
          .panel.widget(data-toggle="play-animation", data-play="fadeInRight", data-offset="0", data-delay="100")
            .panel-body.bg-danger
              .row.row-table.row-flush
                .col-xs-8
                  p.mb0 Ratings received
                  h3.m0 7000
                .col-xs-4.text-right
                  em.fa.fa-star.fa-2x
            .panel-body
              // Bar chart
              .text-center
                .inlinesparkline.inline(data-bar-color="danger", data-height="30", data-bar-width="6", data-bar-spacing="6") 
                  | 2,7,5,9,4,2,7,5,7,5,9,6,4


      // START messages and activity
      .row
        .col-md-6
          .panel.panel-default
            .panel-heading
              .pull-right.label.label-danger 5
              .pull-right.label.label-success 12
              .panel-title Messages
            // START list group
            .list-group
              // START list group item
              a.list-group-item(href='#')
                .media
                  .pull-left
                    img.media-object.img-circle.thumb48(src="app/img/user/01.jpg", alt="Image")
                  .media-body.clearfix
                    small.pull-right 2h
                    strong.media-heading.text-primary 
                      span.circle.circle-success.circle-lg.text-left
                      | Jean Daniels
                    p.mb-sm: small Cras sit amet nibh libero, in gravida nulla. Nulla...
              // END list group item
              // START list group item
              a.list-group-item(href='#')
                .media
                  .pull-left
                    img.media-object.img-circle.thumb48(src="app/img/user/04.jpg", alt="Image")
                  .media-body.clearfix
                    small.pull-right 3h
                    strong.media-heading.text-primary 
                      span.circle.circle-success.circle-lg.text-left
                      | Alexis Wright
                    p.mb-sm: small Cras sit amet nibh libero, in gravida nulla. Nulla facilisi.
              // END list group item
              // START list group item
              a.list-group-item(href='#')
                .media
                  .pull-left
                    img.media-object.img-circle.thumb48(src="app/img/user/03.jpg", alt="Image")
                  .media-body.clearfix
                    small.pull-right 4h
                    strong.media-heading.text-primary 
                      span.circle.circle-danger.circle-lg.text-left
                      | Lance Simpson
                    p.mb-sm: small Cras sit amet nibh libero, in gravida nulla. Nulla...
              // END list group item
              // START list group item
              a.list-group-item(href='#')
                .media
                  .pull-left
                    img.media-object.img-circle.thumb48(src="app/img/user/06.jpg", alt="Image")
                  .media-body.clearfix
                    small.pull-right 4h
                    strong.media-heading.text-primary 
                      span.circle.circle-danger.circle-lg.text-left
                      | Krin Price
                    p.mb-sm: small Vestibulum pretium aliquam scelerisque. 
              // END list group item
              // START list group item
              a.list-group-item(href='#')
                .media
                  .pull-left
                    img.media-object.img-circle.thumb48(src="app/img/user/06.jpg", alt="Image")
                  .media-body.clearfix
                    small.pull-right 4h
                    strong.media-heading.text-primary 
                      span.circle.circle-lg.text-left
                      | Johnny Gilbert
                    p.mb-sm: small Sed egestas, augue vitae blandit imperdiet, justo neque tincidunt sapien... 
              // END list group item
            // END list group
            // START panel footer
            .panel-footer.clearfix
              .input-group
                input.form-control.input-sm(type='text', placeholder='Search message ..')
                span.input-group-btn
                  button.btn.btn-default.btn-sm(type='submit')
                    i.fa.fa-search
            // END panel-footer
        .col-md-6
          .panel.panel-default
            .panel-heading
              .panel-title News feed
            // START list group
            .list-group
              // START list group item
              .list-group-item
                .media
                  .pull-left
                    span.fa-stack.fa-lg
                      em.fa.fa-circle.fa-stack-2x.text-purple
                      em.fa.fa-cloud-upload.fa-stack-1x.fa-inverse.text-white
                  .media-body.clearfix
                    .media-heading.text-purple.m0 NEW FILE
                    small.text-muted.pull-right 15 minutes ago
                    p.m0: small
                      | A new uploaded file 
                      a(href="#") Bootstrap.xls 
                      |  is now on the cloud
              // END list group item

              // START list group item
              .list-group-item
                .media
                  .pull-left
                    span.fa-stack.fa-lg
                      em.fa.fa-circle.fa-stack-2x.text-info
                      em.fa.fa-file-text-o.fa-stack-1x.fa-inverse.text-white
                  .media-body.clearfix
                    .media-heading.text-info.m0 NEW DOCUMENT
                    small.text-muted.pull-right 2 hours ago
                    p.m0: small
                      | New document 
                      a(href="#") Bootstrap.doc 
                      |  created
              // END list group item

              // START list group item
              .list-group-item
                .media
                  .pull-left
                    span.fa-stack.fa-lg
                      em.fa.fa-circle.fa-stack-2x.text-danger
                      em.fa.fa-exclamation.fa-stack-1x.fa-inverse.text-white
                  .media-body.clearfix
                    .media-heading.text-danger.m0 IMPORTANT MESSAGE
                    small.text-muted.pull-right 5 hours ago
                    p.m0: small
                      | Todd Walker sent you an important messsage. 
                      a(href="#") Open
              // END list group item

              // START list group item
              .list-group-item
                .media
                  .pull-left
                    span.fa-stack.fa-lg
                      em.fa.fa-circle.fa-stack-2x.text-success
                      em.fa.fa-clock-o.fa-stack-1x.fa-inverse.text-white
                  .media-body.clearfix
                    .media-heading.text-success.m0 NEW MEETING
                    small.text-muted.pull-right 15 hours ago
                    p.m0
                      small 
                        | Roberta Lane added a new meeting. 
                        em Hey Guys! Want to see there tomorrow. We have good new to share with you. Cheers!

              // END list group item
              // START list group item
              .list-group-item
                .media
                  .pull-left
                    span.fa-stack.fa-lg
                      em.fa.fa-circle.fa-stack-2x.text-warning
                      em.fa.fa-envelope-o.fa-stack-1x.fa-inverse.text-white
                  .media-body.clearfix
                    .media-heading.text-warning.m0 ANOTHER MESSAGE
                    small.text-muted.pull-right last week
                    p.m0
                     small Estela Soccer sent you a messsage. Quisque viverra faucibus neque, quis elementum velit vulputate et. 
              // END list group item
            // END list group
            // START panel footer
            .panel-footer.clearfix
              a(href='#').pull-left
                small Load more
            // END panel-footer
      // END messages and activity


    // END dashboard main content

    // START dashboard sidebar
    aside.col-md-3
      // START widget
      .panel.widget
        .panel-body.bg-success.text-center
          p: img.img-circle.thumb64(src='app/img/user/08.jpg',alt="")
          p
            strong Carrie Daniels 
            span is now following you
      // END widget

      // START Secondary Widgets
      // START widget
      .panel.widget
        .panel-body
          .text-right.text-muted
            em.fa.fa-globe.fa-2x
          h3.mt0 50.5 Gb
          p.text-muted Traffic this month
          .progress.progress-striped.progress-xs
            .progress-bar.progress-bar-purple.progress-50(role='progressbar', aria-valuenow='50', aria-valuemin='0', aria-valuemax='100')
              span.sr-only 50% Complete
      // END widget
      // START widget
      .panel.widget
        .panel-body
          .text-right.text-muted
            em.fa.fa-gamepad.fa-2x
          h3.mt0 99.999
          p.text-muted Games played last month
          .progress.progress-striped.progress-xs
            .progress-bar.progress-bar-warning.progress-60(role='progressbar', aria-valuenow='60', aria-valuemin='0', aria-valuemax='100')
              span.sr-only 60% Complete
      // END widget
      // START widget
      .panel.widget
        .panel-body
          .text-right.text-muted
            em.fa.fa-coffee.fa-2x
          h3.mt0 300 
          p.text-muted Coffee cups per day
          .progress.progress-striped.progress-xs
            .progress-bar.progress-bar-green.progress-80(role='progressbar', aria-valuenow='80', aria-valuemin='0', aria-valuemax='100')
              span.sr-only 80% Complete
      // END widget
      // START widget
      .panel.widget
        .panel-body
          .text-right.text-muted
            em.fa.fa-upload.fa-2x
          h3.mt0 1000 Gb
          p.text-muted Average Monthly Uploads
          .progress.progress-striped.progress-xs
            .progress-bar.progress-bar-info.progress-40(role='progressbar', aria-valuenow='40', aria-valuemin='0', aria-valuemax='100')
              span.sr-only 40% Complete
      // END widget
      // END Secondary Widgets

    // END dashboard sidebar
